{
    "extends": "../../tsconfig",
    "compilerOptions": {
        "outDir": "../../build/server",
        "sourceMap": true,
        "module": "commonjs",
        "moduleResolution": "node",
        "baseUrl": "./",
        "paths": {
            "server/*": [
                "./*"
            ],
            "common/*": [
                "../common/*"
            ]
        },
        "types": [
            "node",
            "express"
        ],
        "typeRoots": [
            "./node_modules/@types"
        ]
    },
    "include": [
        "./**/*.ts",
        "../../@types/express.d.ts"
    ],
    "files": []
}